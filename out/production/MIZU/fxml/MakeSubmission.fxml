<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="172.0" prefWidth="473.0" xmlns="http://javafx.com/javafx/16" xmlns:fx="http://javafx.com/fxml/1" fx:controller="fxml.MakeSubmissionController">
   <children>
      <Label layoutX="36.0" layoutY="39.0" text="Product ID:">
         <font>
            <Font size="13.0" />
         </font>
      </Label>
      <Label layoutX="36.0" layoutY="84.0" text="No. of products:" />
      <TextField fx:id="quantityField" layoutX="133.0" layoutY="79.0" prefHeight="26.0" prefWidth="100.0" promptText="Enter quantity" />
      <Button layoutX="331.0" layoutY="132.0" mnemonicParsing="false" onAction="#submitButtonPressed" text="Submit" />
      <Button fx:id="cancelButton" layoutX="395.0" layoutY="132.0" mnemonicParsing="false" onAction="#cancelButtonPressed" text="Cancel" />
      <TextField fx:id="productIDField" layoutX="102.0" layoutY="36.0" prefHeight="26.0" prefWidth="130.0" promptText="Enter valid product ID" />
   </children>
</AnchorPane>
